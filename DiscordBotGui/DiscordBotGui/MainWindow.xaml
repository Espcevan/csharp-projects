<Window x:Class="DiscordBotGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiscordBotGui"
        mc:Ignorable="d"
        Title="MainWindow" Height="411" Width="662" Initialized="Window_Initialized">
    <Grid>
        <DataGrid x:Name="LogGrid" ItemsSource="{Binding}" Height="277" VerticalAlignment="Top" VerticalContentAlignment="Bottom"/>
        <Button Content="Clear Logs" HorizontalAlignment="Left" Margin="10,0,0,54" VerticalAlignment="Bottom" Width="107" Height="35" Click="Button_Click"/>
        <Button Content="TestAddLog" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="107" Height="39" Click="Button_Click_1"/>
    </Grid>
</Window>
